<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Menu.Menu"
    android:background="@color/colorBg"
    >

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="64dp"
        app:srcCompat="@mipmap/leceladon_managing3"
        tools:layout_editor_absoluteX="130dp"
        tools:layout_editor_absoluteY="86dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_below="@+id/imageView3"
        android:layout_centerHorizontal="true"
        app:srcCompat="@mipmap/cldmang"
        tools:layout_editor_absoluteX="130dp"
        tools:layout_editor_absoluteY="163dp" />

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"

        android:layout_marginBottom="57dp">

        <RelativeLayout
            android:id="@+id/relativeLInventory"
            android:layout_width="match_parent"
            android:layout_height="74dp"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="195dp"
            android:layout_toStartOf="@+id/imageView3"
            android:clickable="true"
            tools:layout_editor_absoluteX="53dp"
            tools:layout_editor_absoluteY="271dp">

            <TextView
                android:id="@+id/textView16"
                style="@style/labelinfo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:text="Inventaire"
                tools:layout_editor_absoluteX="71dp"
                tools:layout_editor_absoluteY="335dp" />

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="44dp"
                android:layout_height="48dp"
                android:layout_centerInParent="true"
                app:layout_constraintBottom_toTopOf="@+id/textView16"
                app:srcCompat="@mipmap/inventorylogo"
                tools:layout_editor_absoluteX="71dp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relativeLDelivry"
            android:layout_width="69dp"
            android:layout_height="79dp"
            android:layout_alignTop="@+id/relativeLInventory"
            android:layout_toEndOf="@+id/imageView3"
            android:clickable="true"
            tools:layout_editor_absoluteX="137dp"
            tools:layout_editor_absoluteY="271dp">

            <TextView
                android:id="@+id/textView15"
                style="@style/labelinfo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:text="Livraison"
                tools:layout_editor_absoluteX="71dp"
                tools:layout_editor_absoluteY="335dp" />

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="44dp"
                android:layout_height="48dp"
                android:layout_centerInParent="true"
                app:srcCompat="@mipmap/delivrylogo"
                tools:layout_editor_absoluteX="71dp" />
        </RelativeLayout>
    </FrameLayout>

    <TextView
        android:id="@+id/textView10"
        style="@style/menuTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Menu"
        tools:layout_editor_absoluteX="63dp"
        tools:layout_editor_absoluteY="25dp" />

</RelativeLayout>